{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "darkcircuit",
  "version": "0.1.0",
  "identifier": "com.darkcircuit.app",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://127.0.0.1:8000",
    "beforeBuildCommand": "",
    "frontendDist": "public"
  },
  "app": {
    "windows": [
      {
        "title": "darkcircuit",
        "width": 1600,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "backend_server"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "all": true
    }
  }
}
